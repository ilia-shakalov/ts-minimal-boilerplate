  },
  "devDependencies": {
    "@types/node": "20.11.0",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-filename-rules": "1.3.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-unused-imports": "3.0.0",
    "prettier": "3.2.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
